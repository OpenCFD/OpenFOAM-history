FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "templates";
    object      outletOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

returnFlow
{
    default
    {
        p_rgh
        {
            type        fixedValue;
            value       ${:VALUE.p_rgh};
        }
    }
    wall
    {
        p_rgh
        {
            type        fixedValue;
            value       ${:VALUE.p_rgh};
        }
    }
    atmosphere
    {
        p_rgh
        {
            type        totalPressure;
            U           U;
            phi         phi;
            rho         rho;
            psi         none;
            gamma       1;
            p0          ${:outlet.p_rgh};
            value       ${:outlet.p_rgh};
        }
    }
}

