FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "templates";
    object      outlet;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

returnFlow
{
    default
    {
        U
        {
            type        pressureInletOutletVelocity;
            value       ${:VALUE.U};
        }
    }
    wall
    {
        U
        {
            type        inletOutlet;
            inletValue  uniform (0 0 0);
            value       ${:VALUE.U};
        }
    }
}


