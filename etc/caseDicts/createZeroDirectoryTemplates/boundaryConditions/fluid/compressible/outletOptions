FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "templates";
    object      outlet;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

returnFlow
{
    atmosphere
    {
        p
        {
            type        totalPressure;
            U           U;
            phi         phi;
            rho         rho;
            psi         none;
            gamma       1;
            p0          ${:VALUE.p};
            value       ${:VALUE.p};
        }
    }
}


