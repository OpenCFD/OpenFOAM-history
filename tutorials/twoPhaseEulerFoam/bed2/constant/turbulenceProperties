FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    note            "turbulenceProperties for Foam";

    class           dictionary;
    object          turbulenceProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Switch turbulence model on or off

turbulence            off;


// Standard k-epsilon model coefficients
// ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

kEpsilonCoeffs
{
    Cmu                     0.09;
    C1                      1.44;
    C2                      1.92;
    C3                     -0.375;

    alphak                  1.0;
    alphaEps                0.82142;
}


// Wall function coefficients
// ~~~~~~~~~~~~~~~~~~~~~~~~~~

wallFunctionCoeffs
{
    kappa                   0.4187;
    E                       9.0;
}

// ************************************************************************* //
