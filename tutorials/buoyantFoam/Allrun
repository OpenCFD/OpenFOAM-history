#!/bin/sh

currDir=`pwd`
application=`basename $currDir`
cases="hotRoom"
utility=setHotRoom

tutorialPath=`dirname $0`/..
. $tutorialPath/RunFunctions

compileApplication hotRoom $utility

for case in $cases
do
    runApplication blockMesh $case
    runApplication $utility $case
    runApplication $application $case
done
