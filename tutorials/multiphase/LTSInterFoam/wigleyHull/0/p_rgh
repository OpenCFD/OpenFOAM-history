FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            buoyantPressure;
        value           uniform 0;
    }

    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    sides
    {
        type            buoyantPressure;
        value           uniform 0;
    }

    atmosphere
    {
        type            symmetryPlane;
    }

    wigley_solid
    {
        type            buoyantPressure;
        value           uniform 0;
    }
}

// ************************************************************************* //
