FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      Ua;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [ 0 1 -1 0 0 0 0 ];

internalField       uniform (0 0 0);

boundaryField
{
    bottom
    {
        type               fixedValue;
        value              uniform ( 0 0 0 );
    }

    top
    {
        type                inletOutlet;
        inletValue          uniform (0 0 0);
        value               uniform ( 0 0 0 );
    }

    walls
    {
        type               fixedValue;
        value              uniform ( 0 0 0 );
    }

    frontBack
    {
        type               empty;
    }

}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
