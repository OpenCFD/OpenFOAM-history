FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0.25 0);

boundaryField
{
    walls
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    outlet
    {
        type               pressureInletOutletVelocity;
        phi                phi2;
        value              $internalField;
    }
    inlet
    {
        type               fixedValue;
        value              uniform (0 0.25 0);
    }
    frontAndBackPlanes
    {
        type               empty;
    }
}

// ************************************************************************* //
