FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha 0.0
);

regions
(
    boxToCell
    {
        box (0 0 -0.1) (0.15 0.5 0.1);

        fieldValues
        (
            volScalarFieldValue alpha 0.55
        );
    }
);

