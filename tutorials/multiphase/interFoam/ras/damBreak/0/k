FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 0.1;

boundaryField
{
    leftWall
    {
        type            kQRWallFunction;
        value           uniform 0;
    }

    rightWall
    {
        type            kQRWallFunction;
        value           uniform 0;
    }

    lowerWall
    {
        type            kQRWallFunction;
        value           uniform 0;
    }

    atmosphere
    {
        type            inletOutlet;
        inletValue      uniform 0.1;
        value           uniform 0.1;
    }

    defaultFaces
    {
        type            empty;
    }
}


