FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

psiChemistryModel ODEChemistryModel<gasThermoPhysics>;

chemistry       on;

initialChemicalTimeStep 1e-10;

chemistrySolver ode;

odeCoeffs
{
    ODESolver       SIBS;
    eps             1e-04;
}


