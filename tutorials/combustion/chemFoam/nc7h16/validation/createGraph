#!/bin/sh

gnuplot<<EOF
    set terminal postscript eps color enhanced
    set output "OF_vs_CHEMKINII.eps"
    set xlabel "Time / [s]"
    set ylabel "Temperature / [K]"
    set grid
    set key left top
    set xrange [0:0.001]
    set yrange [800:2800]
    plot \
        "../chemFoam.out" u 1:2 t "OpenFOAM" with points lt 1 pt 6,\
        "chemkinII" with lines title "Chemkin II" lt -1
EOF

# ----------------------------------------------------------------- end-of-file

