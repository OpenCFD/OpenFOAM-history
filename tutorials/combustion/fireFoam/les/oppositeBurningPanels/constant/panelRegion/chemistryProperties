FoamFile
{
    version     2.0;
    format      binary;
    class       dictionary;
    location    "constant";
    object      chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

chemistryType
{
    chemistrySolver   ode;
    chemistryThermo   solid;
}

chemistry           on;

initialChemicalTimeStep 1e-07;

odeCoeffs
{
    solver          SIBS;
    eps             0.05;
}

reactions
(
    irreversibleSolidArrheniusReaction
    v  = gas + char
    (7.83e10 15274.57 400 4.86)
);


