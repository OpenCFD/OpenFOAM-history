FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Xi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 0 0 0 0 ];

internalField   uniform 1;

boundaryField
{
    outer
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           uniform 1;
    }
    ground
    {
        type            zeroGradient;
    }
}


