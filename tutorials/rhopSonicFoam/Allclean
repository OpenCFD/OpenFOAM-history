#!/bin/sh

currDir=`pwd`
application=`basename $currDir`
cases="shockTube wedge15Ma5"

tutorialPath=`dirname $0`/..
. $tutorialPath/CleanFunctions

for case in $cases
do
    if [ "$case" = "shockTube" ] 
    then 
        rm -rf $case/0
        cp -r $case/0.org $case/0
    fi 
    cleanCase $case
done 


