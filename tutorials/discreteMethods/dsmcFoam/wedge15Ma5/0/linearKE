FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      linearKE;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    flow
    {
        type            zeroGradient;
    }

    obstacle
    {
        type            calculated;
        value           uniform 0;
    }

    periodic
    {
        type            cyclic;
    }
}

