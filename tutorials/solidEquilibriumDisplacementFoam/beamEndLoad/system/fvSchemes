FoamFile
{
    version  2.0;
    format   ascii;

    root     "";
    case     "";
    instance "";
    local    "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    default     steadyState;
}

gradSchemes
{
    default         Gauss cubic; //fourth; //Gauss linear;
}

divSchemes
{
    default         Gauss cubic; //Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear uncorrected;
}

interpolationSchemes
{
    default         cubic;
}

snGradSchemes
{
    default         uncorrected;
}

fluxRequired
{
    default         no;
    D;
}

