    Cmu              0.0845;
    // C1
    C1               1.42;
    // C2
    C2               1.68;
    // C3
    C3               -0.33;
    // alphah
    alphah           1;
    // alphak
    alphak           1.39;
    // alphaEps
    alphaEps         1.39;
    // eta0
    eta0             4.38;
    // beta
    beta             0.012;
}

