FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          turbulenceProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Turbulence model selection
RASmodel            kEpsilon;

// Do you wish to calculate turbulence?
turbulence      on;

// Laminar model coefficients
laminarCoeffs
{
}

// Standard k-epsilon model coefficients
kEpsilonCoeffs
{
    // Cmu
    Cmu              0.09;
    // C1
    C1               1.44;
    // C2
    C2               1.92;
    // C3
    C3               -0.33;
    // alphah
    alphah           1;
    // alphak
    alphak           1;
    // alphaEps
    alphaEps         0.76923;
}

// RNG k-epsilon model coefficients
