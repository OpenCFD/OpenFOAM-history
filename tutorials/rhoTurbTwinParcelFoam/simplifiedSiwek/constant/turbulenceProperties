    // C1Ref
    C1Ref            0.5;
    // C2Ref
    C2Ref            0.3;
    // Cs
    Cs               0.25;
    // Ceps
    Ceps             0.15;
    // alphah
    alphah           1;
    // alphaEps
    alphaEps         0.76923;
    // alphaR
    alphaR           1.22;
}

// Wall function coefficients
wallFunctionCoeffs
{
