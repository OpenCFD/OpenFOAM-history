FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 4.49e-05;

boundaryField
{
    walls
    {
        type            compressible::epsilonWallFunction;
        value           uniform 4.49e-05;
    }

    inlet
    {
        type            fixedValue;
        value           uniform 4.49e-05;
    }

    outlet
    {
        type            zeroGradient;
    }

    cycLeft
    {
        type            cyclic;
    }

    cycRight
    {
        type            cyclic;
    }

    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
