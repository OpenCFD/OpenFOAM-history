FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system/wallFilmRegion";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(Uf|hsf|deltaf\*rhof)"
    {
        solver          smoothSolver;
        smoother        GaussSeidel;
        tolerance       1e-10;
        relTol          0;
        maxIter         100;
    }
    deltaf
    {
        solver          smoothSolver;
        smoother        GaussSeidel;
        tolerance       1e-10;
        relTol          0;
        maxIter         100;
    }
}


PISO
{
    momentumPredictor true;
    nOuterCorr      1;
    nCorr           1;
    nNonOrthCorr    0;
    dfMin           1e-10;
}


relaxationFactors
{}

