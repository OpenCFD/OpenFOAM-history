FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 -1 0 0 0 0 ];

internalField   uniform 2.6;

boundaryField
{
    movingWall
    {
        type            omegaWallFunction;
        value           uniform 0;
    }

    fixedWalls
    {
        type            omegaWallFunction;
        value           uniform 0;
    }

    frontAndBack
    {
        type            empty;
    }
}


