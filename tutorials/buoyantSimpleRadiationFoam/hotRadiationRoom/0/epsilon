FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volScalarField;
    object          epsilon;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    floor           
    {
        type            zeroGradient;
    }

    ceiling         
    {
        type            zeroGradient;
    }

    fixedWalls      
    {
        type            zeroGradient;
    }

    box
    {
        type            zeroGradient;
    }
}


