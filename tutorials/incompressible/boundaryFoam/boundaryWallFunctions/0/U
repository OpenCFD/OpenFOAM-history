FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 0 0 0 );

boundaryField
{
    lowerWall
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
    upperWall
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
    frontBack_half0
    {
        type            cyclic;
    }
    defaultFaces
    {
        type            empty;
    }
    frontBack_half1
    {
        type            cyclic;
    }
}


