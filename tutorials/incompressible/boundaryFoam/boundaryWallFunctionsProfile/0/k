FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.000000001;

boundaryField
{
    lowerWall
    {
        type            kqRWallFunction;
        internalField   uniform 0.1;
    }
    upperWall
    {
        type            kqRWallFunction;
        internalField   uniform 0.1;
    }
    frontBack
    {
        type            cyclic;
        internalField   uniform 0.1;
    }
    defaultFaces
    {
        type            empty;
    }
}


