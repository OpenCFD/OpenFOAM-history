FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 -1 0 0 0 0 ];

internalField   uniform 1111.11;

boundaryField
{
    lowerWall
    {
        type            omegaWallFunction;
        value           uniform 1111.11;
    }
    upperWall
    {
        type            omegaWallFunction;
        value           uniform 1111.11;
    }
    frontBack_half0
    {
        type            cyclic;
    }
    defaultFaces
    {
        type            empty;
    }
    frontBack_half1
    {
        type            cyclic;
    }
}


