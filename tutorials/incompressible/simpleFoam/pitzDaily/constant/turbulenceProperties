FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType RAS;

RAS
{
    // Tested with ShihQuadraticKE, v2f, kEpsilon, realizableKE,
    // kOmega, kOmegaSST;
    RASModel        v2f;

    turbulence      on;

    printCoeffs     on;
}


