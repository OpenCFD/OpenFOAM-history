FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    movingWall
    {
        type            uniformFixedValue;
        uniformValue    csvFile;
        csvFileCoeffs
        {
            nHeaderLine     1;
            refColumn       0;
            componentColumns (1 2 3);
            mergeSeparators no;
            fileName        "$FOAM_CASE/csvData";
            separator       " ";
        }
    }

    fixedWalls
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
