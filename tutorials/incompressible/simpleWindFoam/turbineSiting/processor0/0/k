FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1.3;

boundaryField
{
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    sides
    {
        type            slip;
    }
    inlet
    {
        type            uniformFixedValue;
        uniformValue    1.3;
    }
    ground
    {
        type            zeroGradient;
    }
    top
    {
        type            slip;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 1.3;
    }
}


