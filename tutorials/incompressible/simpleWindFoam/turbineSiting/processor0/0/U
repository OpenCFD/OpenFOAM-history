FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    sides
    {
        type            slip;
    }
    inlet
    {
        type            atmBoundaryLayerInletVelocity;
        Uref            10;
        Href            20;
        n               ( 1 0 0 );
        z               ( 0 0 1 );
        z0              0.1;
        zGround         935;
        value           uniform (0 0 0);
    }
    ground
    {
        type            uniformFixedValue;
        uniformValue    (0 0 0);
    }
    top
    {
        type            slip;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


