FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -3 0 0 0 0 ];

internalField   uniform 1;

boundaryField
{
    inlet
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.01;
        value           uniform 1;
    }
    outlet1
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           uniform 1;
    }
    outlet2
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           uniform 1;
    }
    baffles
    {
        type            epsilonWallFunction;
        value           uniform 1;
    }
    fan_half0
    {
        type            cyclic;
    }
    defaultFaces
    {
        type            epsilonWallFunction;
        value           uniform 1;
    }
    fan_half1
    {
        type            cyclic;
    }
}


