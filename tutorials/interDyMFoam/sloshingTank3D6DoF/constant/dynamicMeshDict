FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      motionProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dynamicFvMesh solidBodyMotionFvMesh;

solidBodyMotionFvMeshCoeffs
{
    solidBodyMotionFunction SKA;

    SKACoeffs
    {
        // Centre of gravity
        CofG        (0 0 0);

        // Time data file name
        timeDataFileName "constant/6DoF.dat";
    }
}

