FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

porousToair
{
    type            constantHeatTransfer;
    active          on;
    selectionMode   mapRegion;
    interpolationMethod cellVolumeWeight;
    nbrRegionName   air;
    master          true;

    constantHeatTransferCoeffs
    {
        nbrModelName    airToporous;
        fieldNames      (h);
        semiImplicit    no;
    }
}

