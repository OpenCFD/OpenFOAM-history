FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.05;

vertices
(
    (-2 4 4)     // 0
    (12 4 4)     // 1
    (12 6 4)     // 2
    (-2 6 4)     // 3

    (-2 4 6)     // 4
    (12 4 6)     // 5
    (12 6 6)     // 6
    (-2 6 6)     // 7
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (20 4 4) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
    inlet
    {
        type patch;
        faces
        (
            (0 4 7 3)
        );
    }
    outlet
    {
        type patch;
        faces
        (
            (1 2 6 5)
        );
    }
    fixedWalls
    {
        type wall;
        faces
        (
            (0 1 2 3)
            (4 5 6 7)
            (3 2 6 7)
            (0 1 5 4)
        );
    }
);

mergePatchPairs
(
);

