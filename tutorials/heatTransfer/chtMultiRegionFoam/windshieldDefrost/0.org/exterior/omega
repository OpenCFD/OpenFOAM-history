FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0.0001/exterior";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.2;

boundaryField
{
    atmosphere
    {
        type            inletOutlet;
        inletValue      uniform 0.2;
        value           uniform 0.2;
    }
    symmetry
    {
        type            symmetryPlane;
    }
    exterior_to_ice
    {
        type            compressible::omegaWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        beta1           0.075;
        value           uniform 0.2;
    }
}


