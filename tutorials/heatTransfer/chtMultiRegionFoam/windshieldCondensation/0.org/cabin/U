FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0/cabin";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            uniformFixedValue;
        uniformValue           table
        (
            (0   (0 0 0))
            (60  (0 0 0))
            (61  (-2 0 0))
            (100 (-2 0 0))
        );
    }
    outlet
    {
        type            pressureInletOutletVelocity;
        inletValue      uniform (0 0 0);
        value           uniform (0 0 0);
    }
    symmetry
    {
        type            symmetryPlane;
    }
    walls
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    cabin_to_windshield
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


// ************************************************************************* //
