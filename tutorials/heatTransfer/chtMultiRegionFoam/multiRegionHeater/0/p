FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    minX
    {
        type            fixedFluxBuoyantPressure;
        value           1e5;
    }
    maxX
    {
        type            fixedFluxBuoyantPressure;
        value           1e5;
    }

    minY
    {
        type            fixedFluxBuoyantPressure;
        value           1e5;
    }
    maxY
    {
        type            fixedFluxBuoyantPressure;
        value           1e5;
    }
    minZ
    {
        type            fixedFluxBuoyantPressure;
        value           1e5;
    }
    maxZ
    {
        type            fixedFluxBuoyantPressure;
        value           1e5;
    }
}

