FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    floor           
    {
        type            zeroGradient;
    }

    ceiling         
    {
        type            zeroGradient;
    }

    fixedWalls      
    {
        type            zeroGradient;
    }

    box
    {
        type            zeroGradient;
    }
}

