FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volScalarField;
    object          rho;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 -3 1 0 0 1 0];

internalField   uniform 0;

boundaryField
{
    left            
    {
        type            symmetryPlane;
    }

    right           
    {
        type            symmetryPlane;
    }

    down            
    {
        type            symmetryPlane;
    }

    up              
    {
        type            zeroGradient;
    }

    hole            
    {
        type            fixedValue;
        value           uniform 3.57e-05;
    }

    defaultFaces    
    {
        type            empty;
    }
}


