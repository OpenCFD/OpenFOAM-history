FoamFile
{
    version 2.0;
    format ascii;

    root "/home/dm2/henry/OpenFOAM/OpenFOAM-1.4.1/tutorials/electrostaticFoam";
    case "chargedWire";
    instance "constant";
    local "polyMesh";

    class polyBoundaryMesh;
    object boundary;
}

