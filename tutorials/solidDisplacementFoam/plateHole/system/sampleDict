FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          sampleDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


interpolationScheme cellPoint;

setFormat     raw;

sets
(
    leftPatch
    {
        type            uniform;
        axis            y;
        start           (0 0.5 0.25);
        end             (0 2 0.25);
        nPoints         100;
    }
);

surfaces
();

fields
(
    sigmaxx
);

