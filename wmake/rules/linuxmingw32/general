CPP        = /lib/cpp $(GFLAGS)
LD         = mingw32-ld

PROJECT_LIBS = -l$(WM_PROJECT)

include $(GENERAL_RULES)/standard

include $(RULES)/X
include $(RULES)/c
include $(RULES)/c++

SO      = dll
EXE_EXT = .exe

# Ensure we know what OS we are compiling for
# during MakefileFiles and MakefileOptions
GFLAGS  += -DWIN32 -DLITTLE_ENDIAN
