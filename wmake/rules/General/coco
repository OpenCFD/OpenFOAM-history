# handie Coco/R attributed grammars

.SUFFIXES: .atg

atgtoo =  \
    $(WM_THIRD_PARTY_DIR)/coco-cpp/platforms/$(WM_ARCH)/bin/coco-cpp \
    -single \
    -frames $(WM_THIRD_PARTY_DIR)/coco-cpp/platforms/share/coco-cpp \
    $$SOURCE -o $(OBJECTS_DIR) && \
    $(CC) $(c++FLAGS) -c $*.cpp -o $@

.atg.dep:
	$(MAKE_DEP)
