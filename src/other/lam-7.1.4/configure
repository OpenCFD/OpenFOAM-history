    echo "$as_me:$LINENO: result: $LSC_CXX_TEMPLATE_REPOSITORY" >&5
echo "${ECHO_T}$LSC_CXX_TEMPLATE_REPOSITORY" >&6
else
    echo "$as_me:$LINENO: result: not used" >&5
echo "${ECHO_T}not used" >&6
fi
