cat >>conftest.$ac_ext <<_ACEOF
/* end confdefs.h.  */

#include <stdio.h>

int main(int argc, char* argv[])
{
  FILE *fp = fopen("configtest.out", "w");
#ifdef __GLIBC__
  fprintf(fp, "1\n");
#else
