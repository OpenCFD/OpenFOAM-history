    LAM_WANT_CS_FS=1
elif test -z "$with_cs_fs"; then
    LAM_WANT_CS_FS=$have_cs_fs
else
    LAM_WANT_CS_FS=0
fi

if test "$LAM_WANT_CS_FS" = "1"; then
    echo "$as_me:$LINENO: result: yes" >&5
echo "${ECHO_T}yes" >&6
else
    echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6
fi



