    flag="$1"

# 1 is the message
# 2 is whether to put a prefix or not
if test -n ""; then
    echo "configure:27205: examining flag: $flag" >&5
else
    echo examining flag: $flag >&5
fi
    case "$1" in
    $CC) ;;
