	HAVE_MINGW=no
	if test "x$use_shared" = "xyes" ; then
		CXX="$CXX -mthreads"
		CONF_SHARED_CC="$CXX -shared"
	else
		CXXFLAGS="-DBUILD_MICO_DLL $CXXFLAGS"
