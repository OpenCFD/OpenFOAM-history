#!/bin/sh
cd ${0%/*} || exit 1    # run from this directory
set -x

makeOption=libso
[ $# -ge 1 ] && makeOption=$1

wmake $makeOption incompressible
wmake $makeOption interfaceProperties
wmake $makeOption twoPhaseInterfaceProperties
# wmake $makeOption compressible

# ----------------------------------------------------------------- end-of-file
