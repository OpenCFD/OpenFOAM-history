include ../Makefile.in

default:
	(cd IMlib ; $(make) )
	(cd Lib ; $(make) )
	(cd Programs ; $(make) )

clean:
	(cd IMlib ; $(make) clean )
	(cd Lib ; $(make) clean )
	(cd Programs ; $(make) clean )

realclean:
	(cd IMlib ; $(make) realclean )
	(cd Lib ; $(make) realclean )
	(cd Programs ; $(make) realclean )
