#!/bin/sh
cd ${0%/*} || exit 1    # run from this directory
[ $# -ge 1 ] && makeOption=$1 || makeOption=libso
set -x

wmake $makeOption field
wmake $makeOption forces
wmake $makeOption IO
wmake $makeOption utilities
wmake $makeOption jobControl
wmake $makeOption systemCall

# ----------------------------------------------------------------- end-of-file
