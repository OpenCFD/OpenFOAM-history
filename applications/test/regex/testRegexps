// pattern, string
(
    ( "a.*" "abc" )
    ( "a.*" "bac" )
    ( "a.*" "abcd" )
    ( "a.*" "def" )
    ( "d(.*)f" "def" )
    ( " *([A-Za-z]+) *= *([^ /]+) *(//.*)?"   " keyword = value // settings" )
)

