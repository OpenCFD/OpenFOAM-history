// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// #include "codeStreamDefaults"

writeInterval   #codeStream
{
    codeInclude
    #{
        #include "fvCFD.H"
    #};

    codeOptions
    #{
        -I$(LIB_SRC)/finiteVolume/lnInclude
    #};

    code
    #{
        scalar start = 0;
        scalar end = 100;
        label nDumps = 5;
        label interval = end - start;
        Info<<"on-the-fly: " << ((interval)/nDumps) << endl;
        os  << ((interval)/nDumps);
    #};
};


