#!/bin/sh
cd ${0%/*} || exit 1    # run from this directory
set -x

# deal with client/server vs combined plugins
rm -f $FOAM_LIBBIN/libPV398FoamReader* 2>/dev/null

rm -rf PV398FoamReader/Make
wclean libso vtkPV398Foam

