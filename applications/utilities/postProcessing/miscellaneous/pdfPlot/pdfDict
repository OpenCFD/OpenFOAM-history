FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      pdfDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Number of intervals/bins in pdf plot
nIntervals      20;

// Number of samples
nSamples        10000;

// Type of pdf
pdfType         RosinRammler;

RosinRammlerPDF
{
    minValue        1e-06;
    maxValue        200e-06;
    d               (60.0e-06);
    n               (0.8);
}


