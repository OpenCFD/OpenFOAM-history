xMin                          xMin              [0 1 0 0 0]  -100.0;

xMax                          xMax              [0 1 0 0 0]  100.0;

yMin                          yMin              [0 1 0 0 0]  0.0;

yMax                          yMax              [0 1 0 0 0]  0.750;

zMin                          zMin              [0 1 0 0 0]  0.0;

zMax                          zMax              [0 1 0 0 0]  1.0;

insideBoxAlphaValue           insideBoxAlphaValue     [0 0 0 0 0]  0.999;

outsideBoxAlphaValue          outsideBoxAlphaValue     [0 0 0 0 0]  1.0e-6;

