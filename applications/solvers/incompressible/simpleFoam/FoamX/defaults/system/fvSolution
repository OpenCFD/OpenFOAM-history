solvers
{
    p               ICCG 1e-06 0.01;
    U               BICCG 1e-05 0.1;
    k               BICCG 1e-05 0.1;
    epsilon         BICCG 1e-05 0.1;
    R               BICCG 1e-05 0.1;
    nuTilda         BICCG 1e-05 0.1;
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;
}

relaxationFactors
{
    p               0.3;
    U               0.7;
    k               0.7;
    epsilon         0.7;
    R               0.7;
    nuTilda         0.7;
}


